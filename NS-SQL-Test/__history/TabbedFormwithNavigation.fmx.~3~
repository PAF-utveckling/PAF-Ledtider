  object FDQuery1: TFDQuery
    Connection = PafConnection
    SQL.Strings = (
      
        '-- =============================================================' +
        '============='
      
        '-- =============================================================' +
        '============='
      
        '-- SQL fr'#229'ga avsedd att skapa l'#228'mpliga data f'#246'r QlickView fr'#229'n P' +
        'AF databasen'
      '-- Datum: 2016-05-16'
      '-- Utvecklad av: Niklas Storck'
      '-- epost: niklas.storck@capiostgoran.se; niklas@family-storck.se'
      '-- Telefon: 0736-841 220'
      '--'
      '-- Revisioner:'
      '-- Gren f'#246'r DVT endast 2016-05-16'
      '-- Justerad f'#246'r QV'
      
        '-- Bla har koppling till fysweb gjorts f'#246'r att f'#229' med tid f'#246'r e-' +
        'remiss.'
      '-- 0.2.1 Prodtab tillagd'
      '-- 0.2 '#196'ndrad sortering 2015-05-07'
      '-- 0.1 Grund, Niklas Storck 2015-05-04'
      '--'
      
        '-- =============================================================' +
        '============='
      
        '-- =============================================================' +
        '============='
      ''
      'use fyspaf'
      'SELECT '
      
        #9'   REMTAB.REMISSNR as Remissnummer, -- Vill jag ha med f'#246'r sp'#229'r' +
        'barheten till en enskild patient i PAF'
      
        #9'   -- Koll att join fungerar som det ska, f'#246'r min f'#246'rst'#229'else, k' +
        'an tas bort sen'
      #9'    -- PATTAB.REMISSNR, '
      #9'   REMTAB.LEVT as leverant'#246'r,'
      #9'   PRODTAB.PRODGRP as produktGrupp,'
      #9'   REMTAB.PRODKOD as Produktkod,'
      ''
      #9'   PRODTAB.KTXT as Produktbeskrivning,'
      #9'   -- **'
      #9'   REMTAB.MOTT as Kombika,'
      #9'   BESTTAB.NAMN as RemittentKlinik,'
      #9'   BESTTAB.KST  as kostnadsst'#228'lle,'
      ''
      ''
      #9'   -- e-RemissDatum uppdelat p'#229' '#229'r m'#229'nad etc.'
      #9'   PWRemissTab.best_datumtid as eRemissTid,'
      #9'   YEAR(PWRemissTab.best_datumtid) as eRem'#197'r,'
      #9'   MONTH(PWRemissTab.best_datumtid) as eRemM'#229'nad,'
      #9'   DAY(PWRemissTab.best_datumtid) as eRemDag, '
      #9'   DATENAME(hour,PWRemissTab.best_datumtid) as eRemTimme,'
      #9'   DATENAME(MINUTE,PWRemissTab.best_datumtid) as eRemMinut,'
      #9'   DATENAME(weekday,PWRemissTab.best_datumtid) as eRemVeckodag,'
      ''
      ''
      #9'   PATTAB.REMGRANSKDATUMTID as Granskad,'
      #9'   pattab.bokaddatumtid as BokadTid,'
      ''
      #9'   -- RemissDatum uppdelat p'#229' '#229'r m'#229'nad etc.'
      '       PATTAB.REMDATUMTID as RemDatumTid,'
      #9'   YEAR(PATTAB.REMDATUMTID) as Rem'#197'r,'
      #9'   MONTH(PATTAB.REMDATUMTID) as RemM'#229'nad,'
      #9'   DAY(PATTAB.REMDATUMTID) as RemDag, '
      #9'   DATENAME(hour,PATTAB.REMDATUMTID) as RemTimme,'
      #9'   DATENAME(MINUTE,PATTAB.REMDATUMTID) as RemMinut,'
      #9'   DATENAME(weekday,PATTAB.REMDATUMTID) as RemVeckodag,'
      ''
      #9'   -- Signeringsdatum uppdelat p'#229' '#229'r m'#229'nad etc.'
      '       PATTAB.SIGNDATUMTID as Signerad,'
      #9'   YEAR(PATTAB.SIGNDATUMTID) as Sign'#197'r,'
      #9'   MONTH(PATTAB.SIGNDATUMTID) as SignM'#229'nad,'
      #9'   DAY(PATTAB.SIGNDATUMTID) as SignDag, '
      #9'   DATENAME(hour,PATTAB.SIGNDATUMTID) as SignTimme,'
      #9'   DATENAME(MINUTE,PATTAB.SIGNDATUMTID) as SignMinut,'
      #9'   DATENAME(weekday,PATTAB.SIGNDATUMTID) as SignVeckodag,'
      ''
      ''
      #9'   PATTAB.UNDSTARTDATUMTID as UsStart,'
      ''
      '       PATTAB.UNDSLUTDATUMTID as USSlut,'
      #9'  '
